{
  "name": "vaccination-certificate-test-suite",
  "version": "0.0.1",
  "description": "Demonstrates the extensibilty of VCs as immunization certificates.",
  "main": "index.js",
  "scripts": {
    "test": "mocha tests/ --require esm --reporter ./node_modules/mocha-w3c-interop-reporter/index.js --reporter-options body=\"$PWD/body.hbs\",reportDir=\"$PWD/reports\",respec=\"$PWD/respecConfig.json\",title='Vaccination Credentials Interoperability Report 1.0' --timeout 15000",
    "test-node": "mocha tests/ --reporter mochawesome --require mochawesome/register --timeout 15000",
    "generate-certificates": "node generate-certificates.js",
    "fetch-who-int-svc": "if [ ! -e .who-data/svc ]; then git clone --depth 1 https://github.com/who-int/svc.git .who-data/svc; fi",
    "report": "",
    "upload": "aws s3 cp ./reports/ s3://vaccine-interop-report --recursive",
    "lint": "eslint *.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitalbazaar/vaccination-certificate-test-suite.git"
  },
  "keywords": [
    "WHO",
    "Verifiable",
    "Credentials",
    "Immunizations"
  ],
  "author": {
    "name": "Digital Bazaar, Inc.",
    "email": "support@digitalbazaar.com",
    "url": "https://digitalbazaar.com/"
  },
  "bugs": {
    "url": "https://github.com/digitalbazaar/vaccination-certificate-test-suite/issues"
  },
  "homepage": "https://github.com/digitalbazaar/vaccination-certificate-test-suite#readme",
  "dependencies": {
    "@digitalbazaar/http-client": "^1.0.0",
    "@digitalbazaar/vpqr": "github:digitalbazaar/vpqr",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "csv-parse": "^4.15.3",
    "esm": "^3.2.25",
    "mocha": "^8.3.2",
    "mocha-w3c-interop-reporter": "github:digitalbazaar/mocha-w3c-interop-reporter#initial",
    "mochawesome": "^6.2.2",
    "require-dir": "^1.2.0",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": "14.0.0"
  },
  "devDependencies": {
    "eslint": "^7.23.0",
    "eslint-config-digitalbazaar": "^2.6.1",
    "eslint-plugin-jsdoc": "^32.3.0"
  }
}
